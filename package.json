{
  "name": "react-live-drawing-firebase",
  "version": "1.1.5",
  "description": "drawinig canvas save it to firebase",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.js\"",
    "build": "babel src --out-dir lib",
