  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoniopacheco/react-live-drawing-firebase.git"
  },
  "author": "Antonio Pacheco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antoniopacheco/react-live-drawing-firebase/issues"
